{
  "name": "ticker",
  "version": "0.1.0",
  "description": "Ticker manager for Window request animation frame and NodeJS tick",
  "main": "lib/ticker.js",
  "scripts": {
    "start": "webpack-dev-server --progress --config webpack.config.js --open",
    "lint": "eslint ./src --ext .js --cache --fix --ext .jsx --cache --fix",
    "prebuild": "rimraf lib && rimraf demo",
    "build": "./node_modules/.bin/webpack --progress --config webpack.config.js",
    "postbuild": "node scripts/postbuild.js",
    "release:master": "git checkout master && git merge --squash develop && git commit -am 'Release Commit'",
    "release:bugfix": "npm version patch -m 'Bumped patch version number'",
    "release:feature": "npm version minor -m 'Bumped minor version number'",
    "release:breakingchange": "npm version major -m 'Bumped major version number'",
    "postversion": "git push origin HEAD --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StateTree/ticker.git"
  },
  "author": "Sanjay Krishna Anbalagan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StateTree/ticker/issues"
  },
  "homepage": "https://github.com/StateTree/ticker#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.1.1",
    "eslint-loader": "^1.8.0",
    "fs-extra": "^3.0.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.8.2"
  },
  "files": [
    "lib"
  ]
}
